package my.util;
import java.util.*;

class MapTest
{
	public static void main(String[] args) 
	{
		Map list=new HashMap();
		list.put("city","seoul");
		list.put("na","korea");
		list.put("ho","123");
		list.put("na2","korea");

		System.out.println("city:"+list.get("city"));
		
		Iterator i=list.keySet().iterator();
		while( i.hasNext()){
			   Object key=i.next();
			   Object value=list.get(key);
			   //Ã³¸®
			   System.out.println(key+":"+value);
		}

	}
}
